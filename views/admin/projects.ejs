<div class="breadcrumbs">
  <a href="/dashboard/">Dashboard</a> > <a href="/dashboard/projects/">Projects</a>
</div>

<body>
  <div class = "add_button"> 
    <a href="/dashboard/projects/add_project" class="addlink">Add New Project</a>
  </div>
  <table class="mod_table">
    <tr>
      <th>Project Name</th>
      <th>Alerted Users</th>
      <th> </th>
    </tr>
    <% data.forEach((project) => { %>
      <tr>
        <td><%= project.project_id %></td>
        <td><%= project.alerted_users || 'None' %></td>
        <td class="longcol3" style="vertical-align: middle;"><a href="/dashboard/projects/<%= project.project_id %>/edit" class="changelink">Edit Project</a> <a href="/dashboard/projects/<%= project.project_id %>/deleted" onclick="return confirmDelete()" class="deletelink">Delete</a></td>  
      </tr>
    <% }); %>
  </table>
</body>

<!-- JavaScript code for the confirmation dialog -->
<script>
  function confirmDelete() {
    const confirmDialog = confirm('Are you sure you want to delete this item?');
    if (confirmDialog) {
      return true; 
    } else {
      return false; 
    }
  }
  </script>