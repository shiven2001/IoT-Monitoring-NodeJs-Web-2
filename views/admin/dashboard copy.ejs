

<div class="author">
    <h1 class="author__heading"></h1>
    <p class="author__body">NB-IoT Data Portal</p>
  </div>

  <section class="articles">
    <h2 class="articles__heading">Loggers</h2>
  
    <ul class="article-ul">
      <% data.forEach(post => { %>
        <li>
          <a href="/post/<%= post._id %>">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
          </a>
        </li>
      <% }) %>
    </ul>

    <% if (nextPage !== null) { %>
        <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
      <% } %>

  </section> 

